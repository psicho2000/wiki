    "schemes": [],
    "theme": "CustomTheme",
    "themes": [
        {
            "name": "CustomTheme",
            "tab": {
                "background": "#828282FF",
                "iconStyle": "default",
                "showCloseButton": "always",
                "unfocusedBackground": "#272727CC"
            },
            "tabRow": {
                "background": "#333333FF",
                "unfocusedBackground": "#333333CC"
            },
            "window": {
                "applicationTheme": "system",
                "experimental.rainbowFrame": false,
                "frame": "#6F6F6FFF",
                "unfocusedFrame": "#000000CC",
                "useMica": false
            }
        }
    ]
