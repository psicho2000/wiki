{
    "$help": "https://aka.ms/terminal-documentation",
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "actions": [
        {
            "command": {
                "action": "newTab"
            },
            "keys": "ctrl+t"
        },
        {
            "command": "find",
            "keys": "ctrl+shift+f"
        },
        {
            "command": "toggleBlockSelection",
            "keys": "ctrl+shift+b"
        },
        {
            "command": "switchSelectionEndpoint",
            "keys": "ctrl+shift+o"
        },
        {
            "command": {
                "action": "splitPane",
                "split": "auto",
                "splitMode": "duplicate"
            },
            "keys": "alt+shift+d"
        },
        {
            "command": "togglePaneZoom",
            "keys": "alt+shift+z"
        },
        {
            "command": {
                "action": "swapPane",
                "direction": "down"
            },
            "keys": "ctrl+alt+down"
        },
        {
            "command": {
                "action": "swapPane",
                "direction": "left"
            },
            "keys": "ctrl+alt+left"
        },
        {
            "command": {
                "action": "swapPane",
                "direction": "right"
            },
            "keys": "ctrl+alt+right"
        },
        {
            "command": {
                "action": "swapPane",
                "direction": "up"
            },
            "keys": "ctrl+alt+up"
        },
        {
            "command": "unbound",
            "keys": "alt+enter"
        }
    ],
    "alwaysShowNotificationIcon": true,
    "copyFormatting": "all",
    "copyOnSelect": false,
    "defaultProfile": "{17bf3de4-5353-5709-bcf9-835bd952a95e}",
    "firstWindowPreference": "persistedWindowLayout",
    "minimizeToNotificationArea": true,
    "newTabMenu": [
        {
            "type": "remainingProfiles"
        }
    ],
    "startOnUserLogin": false,
    "tabSwitcherMode": "mru",
    "tabWidthMode": "equal",
    "useAcrylicInTabRow": false,
    "profiles": {
        "defaults": {
            "bellStyle": "none",
            "closeOnExit": "always",
            "experimental.repositionCursorWithMouse": true,
            "font": {
                "face": "MonaspiceNe Nerd Font",
                "features": {
                    "calt": 1,
                    "dlig": 1,
                    "ss01": 1,
                    "ss02": 1,
                    "ss03": 1,
                    "ss04": 1,
                    "ss05": 1,
                    "ss06": 1,
                    "ss07": 1,
                    "ss08": 1
                }
            },
            "historySize": 16000,
            "opacity": 100,
            "showMarksOnScrollbar": true,
            "startingDirectory": "%userprofile%",
            "useAcrylic": false
        },
        "list": [
            {
                "background": "#350719",
                "commandline": "\"%PROGRAMFILES%/git/usr/bin/bash.exe\" -i -l",
