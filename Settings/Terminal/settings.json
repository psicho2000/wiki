{
    "$help": "https://aka.ms/terminal-documentation",
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "actions": [
        {
            "command": {
                "action": "newTab"
            },
            "keys": "ctrl+t"
        },
        {
            "command": "find",
            "keys": "ctrl+shift+f"
        },
        {
            "command": "switchSelectionEndpoint",
            "keys": "ctrl+shift+o"
        },
        {
            "command": "toggleBlockSelection",
            "keys": "ctrl+shift+b"
        },
        {
            "command": {
                "action": "splitPane",
                "split": "auto",
                "splitMode": "duplicate"
            },
            "keys": "alt+shift+d"
        },
        {
            "command": "togglePaneZoom",
            "keys": "alt+shift+z"
        },
        {
            "command": {
                "action": "swapPane",
                "direction": "down"
            },
            "keys": "ctrl+alt+down"
        },
        {
            "command": {
                "action": "swapPane",
                "direction": "left"
            },
            "keys": "ctrl+alt+left"
        },
        {
            "command": {
                "action": "swapPane",
                "direction": "right"
            },
            "keys": "ctrl+alt+right"
        },
        {
            "command": {
                "action": "swapPane",
                "direction": "up"
            },
            "keys": "ctrl+alt+up"
        },
        {
            "command": "unbound",
            "keys": "alt+enter"
        }
    ],
    "alwaysShowNotificationIcon": true,
    "copyFormatting": "all",
    "copyOnSelect": false,
    "defaultProfile": "{17bf3de4-5353-5709-bcf9-835bd952a95e}",
    "minimizeToNotificationArea": true,
    "startOnUserLogin": false,
    "tabSwitcherMode": "mru",
    "useAcrylicInTabRow": true,
